{
    "manifest_version": 3,
    "name": "Auto Fetch Questions and Process Answers",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://azota.vn/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "./logo/icon.png",
        "48": "./logo/icon.png",
        "128": "./logo/icon.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}